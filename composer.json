{
    "name": "smousss/laravel-novalize",
    "description": "Generate Laravel Nova admin panels at light speed using GPT-4!",
    "keywords": [
        "admin",
        "admin panel",
        "ai",
        "artificial intelligence",
        "chatgpt",
        "gpt-3",
        "gpt-4",
        "laravel nova",
        "laravel",
        "openai",
        "smousss novalize",
        "smousss"
    ],
    "homepage": "https://github.com/smousss/laravel-novalize",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Crozat",
            "email": "hello@benjamincrozat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Smousss\\Laravel\\Novalize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smousss\\Laravel\\Novalize\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smousss\\Laravel\\Novalize\\NovalizeServiceProvider"
            ],
            "aliases": {
                "Novalize": "Smousss\\Laravel\\Novalize\\Facades\\Novalize"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
